<%= form_for @project do |f| %>
    <%= render "shared/error_messages", object: f.object %>
    <%= f.label :title, style: 'width: 100px;' %>
    <%= f.text_field :title , style: 'width: 200px;'%><br/>
    <%= f.label :description , style: 'width: 100px;'%>
    <%= f.text_area :description , style: 'width: 200px;'%>
    <br/>
    <br/>
    <%= submit_tag %>
<% end %>